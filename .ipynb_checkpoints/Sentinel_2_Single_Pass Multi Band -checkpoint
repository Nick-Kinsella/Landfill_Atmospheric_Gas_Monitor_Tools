    "# Plotting the active B11 band\n",
    "plt.figure(figsize=(10, 10))\n",
    "plt.imshow(active_frac_change_cropped, cmap='viridis')  # You can choose any colormap you prefer\n",
    "plt.colorbar(label='Reflectance')\n",
    "plt.title('Active Emission Band B12')\n",
    "plt.xlabel('Column #')\n",
    "plt.ylabel('Row #')\n",
    "plt.show()"
   ]
